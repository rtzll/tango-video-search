{
	"name": "tango-video-search",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"packageManager": "bun@1.3.5",
	"scripts": {
		"build": "bunx --bun --no-install react-router build",
		"dev": "bunx --bun --no-install react-router dev",
		"lint": "bunx --bun --no-install biome check",
		"lint:fix": "bunx --bun --no-install biome check --write",
		"format": "bunx --bun --no-install biome format --write",
		"start": "bunx --bun --no-install react-router-serve ./build/server/index.js",
		"typecheck": "bunx --bun --no-install react-router typegen && bunx --bun --no-install tsc",
		"knip": "bunx --bun --no-install knip"
	},
	"dependencies": {
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/themes": "^3.2.1",
		"@react-router/node": "^7.12.0",
		"@react-router/serve": "^7.12.0",
		"@tailwindcss/vite": "^4.1.18",
		"drizzle-orm": "^0.40.1",
		"isbot": "^5.1.33",
		"radix-themes-tw": "^0.2.3",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-router": "^7.12.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@react-router/dev": "^7.12.0",
		"@types/bun": "^1.3.6",
		"@types/node": "^24.10.9",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"knip": "^5.82.1",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^6.4.1",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"engines": {
		"bun": ">=1.3.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rtzll/tango-video-search.git"
	},
	"license": "BSD-3-Clause"
}
