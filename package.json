{
	"name": "tango-video-search",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"packageManager": "bun@1.3.8",
	"scripts": {
		"build": "bunx --bun --no-install react-router build",
		"dev": "bunx --bun --no-install react-router dev",
		"db:update": "bun scripts/update-production-db.ts",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"format": "biome format --write",
		"start": "bunx --bun --no-install react-router-serve ./build/server/index.js",
		"typecheck": "bunx --bun --no-install react-router typegen && tsc",
		"knip": "knip"
	},
	"dependencies": {
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/themes": "^3.3.0",
		"@react-router/node": "^7.13.0",
		"@react-router/serve": "^7.13.0",
		"@tailwindcss/vite": "^4.1.18",
		"drizzle-orm": "^0.40.1",
		"isbot": "^5.1.35",
		"radix-themes-tw": "^0.2.3",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-router": "^7.13.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.15",
		"@react-router/dev": "^7.13.0",
		"@types/bun": "^1.3.9",
		"@types/node": "^24.10.13",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"knip": "^5.83.1",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^6.4.1",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"engines": {
		"bun": ">=1.3.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rtzll/tango-video-search.git"
	},
	"license": "BSD-3-Clause"
}
